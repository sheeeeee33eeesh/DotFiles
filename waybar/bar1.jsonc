{
   "layer": "top",
   "position": "top",
   "margin-top": 5,
   "margin-bottom": 5,
   "margin-left": 5,
   "margin-right": 5,
   "height": 35,
   "spacing": 7,
   "interval": 1,
   "modules-left": [ "hyprland/workspaces"
   ],
   "modules-center": [
       "group/netadapters",
       "network#speed",
       "group/performance"
   ],
   "modules-right": [
       "backlight",
       "battery"
   ],
   "hyprland/workspaces": {
       "disable-scroll": false,
       "all-outputs": true,
       "warp-on-scroll": false,
       "format": "{icon}",
       "format-icons": {
           "1": "I",
           "2": "II",
           "3": "III",
           "4": "IV",
           "5": "V",
           "6": "VI",
           "7": "VII",
           "8": "VIII",
           "9": "IX",
           "10": "X",
       }
   },
   "group/netadapters": {
      "orientation": "horizontal",
      "modules": [
         "network#tailscale0",
         "network#wlan0",
         "network#enp2s0f3u1"
      ]
   },
   "network#enp2s0f3u1": {
       "interface": "enp2s0f3u1",
       "format-ethernet": "{ifname}: {ipaddr}/{cidr}",
       "format-linked": "{ifname}: (No IP)",
       "format-disconnected": "enp2s0f3u1: Disconnected",
   },
   "network#tailscale0": {
       "interface": "tailscale0",
       "format-ethernet": "{ifname}: {ipaddr}/{cidr}",
       "format-linked": "{ifname}: (No IP)",
       "format-disconnected": "{ifname}: Disconnected",
   },
   "network#wlan0": {
       "interface": "wlan0",
       "format-wifi": "{ifname}: {ipaddr}/{cidr}‚Üí{essid} ({signalStrength}%) Ôá´ ",
       "format-linked": "{ifname}: (No IP)",
       "format-disconnected": "{ifname}: Disconnected"
   },
   "network#speed": {
      "interval": 1,
      "format": "{ifname}",
      "format-wifi": "{icon} ÔÑ∫ {bandwidthDownBytes} ÔÑπ {bandwidthUpBytes} ",
      "format-ethernet": "{icon} ÔÑ∫ {bandwidthDownBytes} ÔÑ∫ {bandwidthUpBytes} ",
      "format-disconnected": "Û∞åô",
      "tooltip-format": "{ipaddr}",
      "format-linked": "Û∞àÅ {ifname} (No IP)",
      "tooltip-format-wifi": "{essid} {icon} {signalStrength}%",
      "tooltip-format-ethernet": "{ifname} Û∞åò",
      "tooltip-format-disconnected": "Û∞åô Disconnected",
      "max-length": 30,
      "format-icons": ["Û∞§Ø","Û∞§ü","Û∞§¢","Û∞§•","Û∞§®"]
   },
   "group/performance": {
      "orientation": "horizontal",
      "modules": [
         "cpu",
         "memory",
         "disk"
      ]
   },
   "cpu": {
       "format": "Ôãõ {usage}%",
       "tooltip": false
   },
   "memory": {
       "format": "ÔÉâ {avail}GB"
   },
   "disk": {
       "format": "üñ¥ {free}",
       "path": "/"
   },
   "backlight" : {
      "format": "üîÜ {}%"
   },
   "battery": {
       "states": {
           "good": 95,
           "warning": 30,
           "critical": 15
       },
       "format": "{capacity}% {icon}",
       "format-full": "{capacity}% {icon}",
       "format-charging": "{capacity}% Ôóß",
       "format-plugged": "{capacity}% Ôá¶",
       "format-alt": "{time} {icon}",
       "format-icons": ["ÔâÑ", "ÔâÉ", "ÔâÇ", "ÔâÅ", "ÔâÄ"]
   }
}
